package com.weiweiqin.model;

import java.io.Serializable;

public class ColorSizeItem implements Serializable {

	private static final long serialVersionUID = -5124015840148732157L;

	private int id;

	 private int colorId;
	 
	 private int sizeId;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getColorId() {
		return colorId;
	}

	public void setColorId(int colorId) {
		this.colorId = colorId;
	}

	public int getSizeId() {
		return sizeId;
	}

	public void setSizeId(int sizeId) {
		this.sizeId = sizeId;
	}

}