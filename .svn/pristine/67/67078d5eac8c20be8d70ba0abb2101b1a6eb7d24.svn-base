#wforder { width: 500px; height: 480px; overflow: hidden; padding: 0px; border: 0px solid rgb(0, 204, 204); float: right; margin-right: 60px; }
.bdbox li { width: 520px; height: 30px; line-height: 20px; padding: 4px 0px 0px; border-bottom: 0px solid rgb(26, 26, 26); display: table-row; }
.bdbox li input.dxk { display: block; float: left; width: 15px; height: 15px; color: rgb(255, 0, 0); padding: 1px 1px 0px 0px; cursor: pointer; background-color: rgb(230, 230, 230); }
.bdbox li label { display: block; float: left; height: 18px; line-height: 18px; background-color: rgb(255, 255, 255); margin: 0px 20px 0px 6px; cursor: pointer; }
.bdbox li span.bdl { float: left; width: 80px; line-height: 24px; text-align: right; }
.bdbox li span.bdr { float: right; width: 400px; padding-left: 7px; border-left: 1px solid rgb(255, 255, 255); margin-left: 7px; }
.bdbox li.product { height: 30px; }
.bdbox li.pay { height: 70px; }
.bdbox li.guest { height: 64px; }
.bdbox li.code { height: 30px; border: 0px none; }
.bdbox li span.bdr .payaa { height: 25px; padding-top: 3px; }
.bdbox li span.bdr .paybb { height: 25px; color: rgb(189, 0, 0); padding: 8px 6px 0px; border: 1px dotted rgb(204, 204, 204); background-color: rgb(249, 249, 249); }
.bdbox li span.bdr input.txt { height: 22px; line-height: 22px; vertical-align: middle; font-size: 12px; color: rgb(0, 0, 0); padding: 0px 6px; outline: 0px none; border: 1px solid rgb(200, 200, 200); background: url('../images/putbg.jpg') no-repeat scroll 0% 0% transparent; }
.bdbox li span.bdr textarea.guest { height: 40px; font-size: 12px; color: rgb(0, 102, 0); padding: 6px; border: 1px solid rgb(200, 200, 200); }
.bdbox li input.send { width: 85px; height: 28px; padding: 2px 0px 0px; color: rgb(204, 0, 0); vertical-align: middle; cursor: pointer; border: 1px solid rgb(187, 187, 187); }
.bdbox li span.bdl01 { float: left; width: 80px; line-height: 24px; text-align: right; }
.bdbox li span.bdr01 { float: right; width: 400px; padding-left: 7px; border-left: 1px solid rgb(255, 255, 255); margin-left: 7px; }
