/**
 * begin mask css
 */
.loading-indicator {
	height: 80px;
	width: 80px;
	background: url( '../image/loading.gif' );
	background-repeat: no-repeat;
	background-position: center center;
	z-index: 99999;
	position: absolute;
}

.loading-overlay {
	background-color: #FFFFFF;
	opacity: 0.6;
	filter: alpha(opacity =   60);
	z-index: 99999;
	position: absolute;
}

/**
 * end mask css
 */

/*
 * begin Tips
 */
.SD-tipbox em {
	font-style: normal;
	font-weight: normal;
}

.SD-tipbox {
	background-color: #FFFFE9;
	border: 1px solid #FFDE8E;
	padding: 5px 25px 5px 10px;
	position: absolute;
	z-index: 100;
	font-size: 12px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	-moz-box-shadow: 1px 1px 2px rgba(0, 0, 0, .2);
	-webkit-box-shadow: 1px 1px 2px rgba(0, 0, 0, .2);
	box-shadow: 1px 1px 2px rgba(0, 0, 0, .2);
	display: none;
}

.SD-tipbox .close-ico {
	position: absolute;
	top: 5px;
	right: 5px;
	display: block;
	width: 16px;
	height: 16px;
	background-image: url(http://css.sodao.com/home/img/close-ico.png);
	text-indent: -900px;
	overflow: hidden;
}

.SD-tipbox .close-ico:hover {
	background-position: 0 -16px;
}

.SD-tipbox p {
	line-height: 1.5
}

.SD-tipbox .SD-tipbox-direction {
	position: absolute;
}

.SD-tipbox .SD-tipbox-direction em,.SD-tipbox .SD-tipbox-direction span
	{
	height: 19px;
	width: 17px;
	font-family: Simsun;
	font-size: 16px;
	line-height: 21px;
	overflow: hidden;
	position: absolute;
}

.SD-tipbox .SD-tipbox-direction em {
	color: #FFDE8E;
}

.SD-tipbox .SD-tipbox-direction span {
	color: #FFFFE9;
}

.SD-tipbox .SD-tipbox-bottom {
	left: 50%;
	margin-left: -8px;
	top: -10px;
	*top: -11px;
	_top: -10px;
}

.SD-tipbox .SD-tipbox-bottom span {
	top: 1px
}

.SD-tipbox .SD-tipbox-top {
	left: 50%;
	margin-left: -8px;
	bottom: 9px;
}

.SD-tipbox .SD-tipbox-top span {
	top: -1px
}

.SD-tipbox .SD-tipbox-left {
	top: 50%;
	margin-top: -8px;
	right: 7px;
}

.SD-tipbox .SD-tipbox-left span {
	left: -1px
}

.SD-tipbox .SD-tipbox-right {
	top: 50%;
	margin-top: -8px;
	left: -8px;
}

.SD-tipbox .SD-tipbox-right span {
	left: 1px
}

.tip-success ,.tip-error,.tip-warn{
	word-break:break-all;
	width:200px;
	margin-left:-100px;
	position:fixed;
	left:50%;
	top:50px;
	overflow: hidden;
	-moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-image: none;
    border-radius: 4px 4px 4px 4px;
    border-style: solid;
    border-width: 1px;
    box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2) inset, 0 1px 2px rgba(0, 0, 0, 0.05);
    display: inline-block;
    font-size: 14px;
    line-height: 20px;
    text-align: center;
    vertical-align: middle;
    z-index: 100;
}



.tip-error {
  color: #ffffff;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #da4f49;
  *background-color: #bd362f;
  background-repeat: repeat-x;
  border-color: #bd362f #bd362f #802420;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffee5f5b', endColorstr='#ffbd362f', GradientType=0);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

.tip-success{
  color: #ffffff;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #5bb75b;
  *background-color: #51a351;
  background-repeat: repeat-x;
  border-color: #51a351 #51a351 #387038;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff62c462', endColorstr='#ff51a351', GradientType=0);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}

.tip-warn{
  color: #ffffff;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #49afcd;
  *background-color: #2f96b4;
  background-repeat: repeat-x;
  border-color: #2f96b4 #2f96b4 #1f6377;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff5bc0de', endColorstr='#ff2f96b4', GradientType=0);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}
/*
 * end Tips
 */

/**
 * begin 图片居中
 */
.img-lazy-box { /*非IE的主流浏览器识别的垂直居中的方法*/
	display: table-cell;
	vertical-align: middle;
	/*设置水平居中*/
	text-align: center;
	/* 针对IE的Hack */
	*display: block;
	*font-size: 175px; /*约为高度的0.873，200*0.873 约为175*/
	*font-family: Arial; /*防止非utf-8引起的hack失效问题，如gbk编码*/
	border: 1px solid #333333;
}

.img-lazy-box img { 
	vertical-align: middle;
}

/**
*end 图片居中
*/