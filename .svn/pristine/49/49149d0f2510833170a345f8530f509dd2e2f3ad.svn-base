package com.weiweiqin.model;

import java.io.Serializable;

public class GoodsPicItem implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = -400055590749272336L;

	private int id;
	
	private int goodsId;
	
	private int picId;
	
	/*
	 * 0:详情图片，1：封面图片
	 */
	private int type;
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(int goodsId) {
		this.goodsId = goodsId;
	}

	public int getPicId() {
		return picId;
	}

	public void setPicId(int picId) {
		this.picId = picId;
	}
	
	public String toString() {

		StringBuffer sb = new StringBuffer();
		sb.append("id=");
		sb.append(id);
		sb.append(":goodsId=");
		sb.append(goodsId);
		sb.append(":picId=");
		sb.append(picId);
		return sb.toString();
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

}
